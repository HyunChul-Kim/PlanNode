<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CalendarPagerView">
        <attr name="dayHeight" format="float"/>
    </declare-styleable>
    <declare-styleable name="CalendarView">
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="date" format="string"/>
    </declare-styleable>
    <declare-styleable name="MonthView">
        <attr name="month_height" format="dimension|reference"/>
        <attr name="day_of_week_height" format="dimension|reference"/>
    </declare-styleable>
</resources>